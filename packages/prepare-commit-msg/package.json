{
  "name": "@coauthors/prepare-commit-msg",
  "version": "0.0.2",
  "description": "Make us as co-author easily",
  "keywords": ["github", "co-author", "git", "collaboration"],
  "homepage": "https://github.com/coauthors/coauthors",
  "bugs": "https://github.com/coauthors/coauthors/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/coauthors/coauthors.git",
    "directory": "packages/prepare-commit-msg"
  },
  "license": "MIT",
  "author": {
    "name": "Jonghyeon Ko",
    "email": "manudeli.ko@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "@coauthors/prepare-commit-msg": "./dist/index.js"
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "ci:attw": "attw --pack",
    "ci:publint": "publint --strict",
    "ci:type": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "vitest run --coverage --typecheck",
    "test:watch": "vitest --ui --coverage --typecheck"
  },
  "dependencies": {
    "@coauthors/core": "workspace:0.0.2"
  },
  "devDependencies": {
    "@coauthors/tsconfig": "workspace:*",
