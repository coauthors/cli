{
  "name": "@coauthors/cli",
  "version": "0.0.2",
  "description": "A utility tool to simplify the process of adding co-authors to GitHub commits, streamlining collaboration directly from the command line.",
  "keywords": ["github", "co-authoring", "git", "collaboration", "cli"],
  "homepage": "https://github.com/coauthors/coauthors",
  "bugs": "https://github.com/coauthors/coauthors/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/coauthors/coauthors.git",
    "directory": "packages/cli"
  },
  "license": "ISC",
  "author": {
    "name": "2-NOW",
    "email": "kj109888@gmail.com"
  },
  "type": "commonjs",
  "main": "src/index.ts",
  "bin": {
    "@coauthors/cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "ci:attw": "attw --pack",
    "ci:publint": "publint --strict",
    "ci:type": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "prepack": "pnpm build",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@coauthors/core": "workspace:0.0.2"
  },
  "devDependencies": {
    "@coauthors/tsconfig": "workspace:*",
    "@coauthors/tsup": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
