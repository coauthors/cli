{
  "name": "coauthors-repo",
  "version": "0.0.0",
  "private": true,
  "description": "Make us as Co-author easily",
  "keywords": [
    "github",
    "coauthors",
    "git",
    "collaboration",
    "cli",
    "prepare-commit-msg",
    "vscode",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coauthors/coauthors.git"
  },
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "build": "turbo run build",
    "build:watch": "turbo run build:watch --parallel",
    "changeset": "changeset",
    "changeset:publish": "pnpm prepack && changeset publish",
    "changeset:version": "changeset version && pnpm i --lockfile-only",
    "ci:all": "pnpm ci:attw && pnpm ci:publint && pnpm ci:sherif && pnpm ci:type && pnpm test && pnpm build",
    "ci:attw": "turbo run ci:attw",
    "ci:publint": "turbo run ci:publint",
    "ci:sherif": "sherif",
    "ci:type": "turbo run ci:type",
    "clean": "turbo run clean",
    "coauthors": "pnpm --filter='coauthors' prepack && packages/coauthors/dist/index.js",
    "dev": "turbo run dev",
    "extension:compile": "pnpm --filter='@coauthors/vscode' compile",
    "graph": "rimraf ./graph && mkdir graph && turbo run build --graph=graph/index.html",
    "packlint": "packlint sort -R",
    "preinstall": "corepack enable",
    "prepack": "turbo run prepack",
    "prepare": "husky",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch --parallel"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@biomejs/biome": "1.6.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.12.7",
    "@vitest/coverage-istanbul": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "packlint": "^0.2.4",
    "publint": "^0.2.7",
    "rimraf": "^5.0.5",
    "sherif": "^0.8.1",
    "tsup": "^8.0.2",
    "turbo": "^2.0.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  }
}
